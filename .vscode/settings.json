{
  "command-runner.commands": {
    "create env": "conda create -n retraced-demo python=3.12 -y",
    "activate env": "conda activate retraced-demo",
    "install requirements": "pip install -r requirements.txt",
    "feed gpt": "python gpt-feeder/feeder.py --root ./ --ignore ./.git/ ./gpt-feeder/ LICENSE .gitmodules",

    "retraced": "python src/retraced.py --input test-imgs/sunset-digi.jpg --height 3000 --samples 200 --lambda_scale 3.0 --grain_radius 0.01 --grain_sigma 0.002 --sigma_filter 0.02 --color",
    "scanner": "python src/scanner.py"
  },
  "python.analysis.diagnosticSeverityOverrides": {
    "reportInvalidTypeForm": "none"
  }
}
