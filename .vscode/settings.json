{
  "command-runner.commands": {
    "create env": "conda create -n retraced-demo python=3.12 -y",
    "activate env": "conda activate retraced-demo",
    "install requirements": "pip install -r requirements.txt",
    "feed gpt": "python gpt-feeder/feeder.py --root ./src/",

    "retraced": "python src/retraced.py --input test-imgs/1.jpg --height 3000 --samples 200 --lambda_scale 3.0 --grain_radius 0.2 --grain_sigma 0.05 --sigma_filter 0.4",
    "scanner": "python src/scanner.py"
  },
  "python.analysis.diagnosticSeverityOverrides": {
    "reportInvalidTypeForm": "none"
  }
}
